#include <stdio.h>
#include <stdlib.h>
#include <string.h>

// Define the EEPROM base address
#define EEPROM_BASE 0x4000
// Define the EEPROM size
#define EEPROM_SIZE 0x1000
// Define the EEPROM control register address
#define EEPROM_CTRL_REG 0x4000
// Define the EEPROM data register address
#define EEPROM_DATA_REG 0x4001
// Define the EEPROM status register address
#define EEPROM_STATUS_REG 0x4002
// Define the EEPROM address register address
#define EEPROM_ADDR_REG 0x4003
// Define the EEPROM write enable bit
#define EEPROM_WRITE_ENABLE 0x01
// Define the EEPROM read enable bit
#define EEPROM_READ_ENABLE 0x02
// Define the EEPROM erase enable bit
#define EEPROM_ERASE_ENABLE 0x04
// Define the EEPROM busy bit
#define EEPROM_BUSY 0x08
// Define the EEPROM error bit
#define EEPROM_ERROR 0x10

void writeEEPROM(unsigned int address, unsigned char data);
unsigned char readEEPROM(unsigned int address);
void eraseEEPROM(unsigned int address);
void printEEPROM();
int main();

